{
  "name": "dcl-project",
  "version": "1.0.0",
  "description": "My new Decentraland project",
  "scripts": {
    "start": "dcl start",
    "build": "build-ecs",
    "build:ci": "dcl build",
    "deploy:prod": "dcl deploy",
    "deploy:testing": "dcl deploy --target peer-testing.decentraland.org",
    "deploy:world": "dcl deploy --target-content https://worlds-content-server.decentraland.org",
    "watch": "build-ecs --watch",
    "deploy:now": "dcl export && now export",
    "ecs:install": "npm install --save-dev decentraland-ecs@latest",
    "ecs:install-next": "npm install --save-dev decentraland-ecs@latest"
  },
  "dependencies": {
    "@dcl/ecs-scene-utils": "^1.7.5",
    "@dcl/ui-scene-utils": "^1.8.4",
    "eth-connect": "^6.0.0"
  },
  "devDependencies": {
    "decentraland-ecs": "^6.11.14"
  },
  "bundledDependencies": [
    "@dcl/ui-scene-utils",
    "eth-connect",
    "@dcl/ecs-scene-utils"
  ]
}